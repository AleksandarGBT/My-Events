<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myevents">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/tv"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tv"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyEvents">
        <activity android:name=".WebsiteActivity"></activity>
        <activity android:name=".TrainingActivity"
            android:label="Event"/>
        <activity android:name=".PlanActivity"
            android:label="My Plans"/>
        <activity android:name=".EditActivity"
            android:label="Edit"/>
        <activity android:name=".AllTrainingsActivity"
            android:label="All Events"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>